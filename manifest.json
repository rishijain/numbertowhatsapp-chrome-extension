{
  "manifest_version": 2,
  "name": "Whatsapp Chrome Plugin",
  "description": "Whatsapp Plugin to send messages without saving the number",
  "version": "1.0.0",
  "content_scripts":[
    {
      "matches":[
        "https://www.youtube.com/*"
      ],
      "js": ["content.js"]
    }
  ],
  "permissions": [
    "activeTab",
    "storage",
    "https://ajax.googleapis.com/"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
   "default_icon": {
     "16": "icon16.png",
     "32": "icon32.png"
   }
 }
}